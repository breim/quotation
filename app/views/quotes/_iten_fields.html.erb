<div class="nested-fields">
	<table class="table">
		<thead>
			<tr>
				<th>Name</th>
				<th>amount</th>
				<th>Part number</th>
				<th> </th>
			</tr>
		</thead>
		<tbody>
			<tr>
				<td><%= f.text_field :name,:class => "form-control" %></td>
				<td><%= f.text_field :amount,:class => "form-control" %></td>
				<td><%= f.text_field :part_number,:class => "form-control" %></td>
				<td><%= link_to_remove_association "<i class='fa fa-minus-circle'></i>".html_safe, f ,:class => 'btn btn-danger'%></td>
			</tr>
		</tbody>
	</table>
</div>